{
  "name": "oidc-connector",
  "version": "1.0.0-4",
  "description": "oidc client connector",
  "keywords": [
    "keycloak",
    "oidc",
    "oidc-client",
    "openid-connector",
    "oidc-connector"
  ],
  "homepage": "https://github.com/spurreiter/oidc-connector",
  "repository": {
    "type": "git",
    "url": "https://github.com/spurreiter/oidc-connector.git"
  },
  "license": "Apache-2.0",
  "author": "spurreiter",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.cjs"
    },
    "adapter/cordova": {
      "import": "./esm/adapter/cordova.js",
      "require": "./dist/adapter/cordova.cjs"
    },
    "adapter/cordova-native": {
      "import": "./esm/adapter/cordova-native.js",
      "require": "./dist/adapter/cordova-native.cjs"
    }
  },
  "main": "src/index.js",
  "typings": "src",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "all": "npm t && npm run lint && npm run build",
    "build": "npm run clean && rollup -c",
    "clean": "rm -rf esm/ dist/ coverage/",
    "coverage": "c8 --clean -r text -r html npm t",
    "dev": "webpack serve",
    "dev:server": "nodemon test/support/server.js",
    "example": "webpack && node example/server.js",
    "lint": "eslint --fix \"**/*.js\" \"**/*.cjs\"",
    "test": "mocha"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@webpack-cli/init": "^1.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "c8": "^7.3.5",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.11",
    "cors": "^2.8.5",
    "debug": "^4.3.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "express": "^4.17.1",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "jsrsasign": "^10.0.5",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.4",
    "sinon": "^9.2.1",
    "superagent": "^6.1.0",
    "supertest": "^6.0.1",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "whatwg-fetch": "^3.4.1"
  },
  "publishConfig": {}
}
